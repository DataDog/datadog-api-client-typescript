{
  "name": "datadog-api-client-typescript",
  "description": "",
  "author": "",
  "keywords": [
    "api",
    "fetch",
    "typescript"
  ],
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/DataDog/datadog-api-client-typescript.git",
    "directory": ""
  },
  "private": true,
  "workspaces": [
    "packages/*",
    "services/*",
    "private/*"
  ],
  "scripts": {
    "check-licenses": "node bin/check-licenses.js",
    "build": "yarn workspaces foreach -Ap run build",
    "lint": "yarn install; eslint --ext .ts packages/* services/*",
    "lint:fix": "yarn install;  yarn lint --fix",
    "format": "yarn install; prettier --write --ignore-unknown '**.{js,ts}'",
    "bdd-test": "yarn workspace bdd-runner run start --working-dir \"$(pwd)\" \"features/v*/*.feature\""
  },
  "devDependencies": {
    "eslint": "^9.23.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "version": "0.0.1",
  "packageManager": "yarn@4.9.1"
}
