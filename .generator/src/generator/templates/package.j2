{
  "name": "{{npmName}}",
  "version": "{{npmVersion}}",
  "description": "OpenAPI client for {{npmName}}",
  "author": "OpenAPI-Generator Contributors",
  "keywords": [
    "fetch",
    "typescript",
    "openapi-client",
    "openapi-generator"
  ],
  "license": "Unlicense",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build"
  },
  {%- if condition %}

  {%- endif %}
  "dependencies": {
    {%- if frameworks %}
      {%- if fetch %}
        {%- if platforms %}
          {%- if node %}
    "node-fetch": "^2.6.0",
    "@types/node-fetch": "^2.5.7",
          {%- endif %}
          {%- if browser %}
    "whatwg-fetch": "^3.0.0",
          {%- endif %}
        {%- endif %}
      {%- endif %}
      {%- if jquery %}
    "@types/jquery": "^3.3.29",
    "jquery": "^3.4.1",
      {%- endif %}
    {%- endif %}
    {%- if platforms %}
      {%- if node %}
    "@types/node": "*",
    "form-data": "^2.5.0",
    "btoa": "^1.2.1",
      {%- endif %}
    {%- endif %}
    {%- if useRxJS %}
    "rxjs": "^6.4.0",
    {%- endif %}
    {%- if useInversify %}
    "inversify": "^5.0.1",
    {%- endif %}
    "es6-promise": "^4.2.4",
    "url-parse": "^1.4.3"
  },
  "devDependencies": {
    "typescript": "^3.9.3"
  }{%- if npmRepository %},{%- endif %}
{%- if npmRepository %}
  "publishConfig":{
    "registry":"{{npmRepository}}"
  }
{%- endif %}
}
